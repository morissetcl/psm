{
  "manifest_version": 2,
  "name": "PSM-CAN",
  "version": "0.1",
  "background": {
    "persistent": false,
    "scripts": [
      "jquery-3.5.0.js",
      "background.js",
      "content.js"
    ]
  },
  "permissions": ["tabs", "*://*/*"],
  "browser_action": {},
  "content_scripts": [
    {
      "matches": [
        "https://duproprio.com/*",
        "https://www.centris.ca/*"
      ],
      "js": ["jquery-3.5.0.js", "content.js"]
    }
  ]
}
